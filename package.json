{
  "name": "uk-72-ui",
  "version": "0.0.1",
  "description": "React SPA for UK72",
  "main": "index.js",
  "scripts": {
    "start": "parallelshell \"npm run -s watch\" ",
    "watch": "watchify src/index.js -dv -t browserify-css -t babelify -o dist/bundle.js",
    "build": "NODE_ENV=production browserify src/index.js -t [ browserify-css --minify=true ] -t babelify -g uglifyify -o dist/bundle.js",
    "test": "NODE_ENV=dev browserify test/*.js -t babelify -t brfs -o dist/test.js && node dist/test.js",
    "standard": "standard --verbose | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidtron/uk-72-ui.git"
  },
  "keywords": [
    "uk",
    "resilience"
  ],
  "author": "David Parry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidtron/uk-72-ui/issues"
  },
  "homepage": "https://github.com/davidtron/uk-72-ui#readme",
  "dependencies": {
    "bootstrap.native": "^0.9.7-npmfix.1",
    "can-use-dom": "^0.1.0",
    "geolib": "^2.0.18",
    "lodash.throttle": "^3.0.4",
    "lscache": "^1.0.5",
    "ospoint": "^0.2.0",
    "react": "^0.14.3",
    "react-addons-update": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-google-maps": "^4.7.0",
    "weather-icons": "^1.3.2",
    "whatwg-fetch": "^0.10.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^12.0.1",
    "browserify-css": "^0.8.4",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "parallelshell": "^2.0.0",
    "snazzy": "^2.0.1",
    "standard": "^5.3.1",
    "tape": "^4.4.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.6.1"
  }
}
